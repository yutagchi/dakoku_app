<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Hello World</title>
  </head>
  <body>
    <a href="/">ホームに戻る</a>
    <ul>
        <% settings.forEach((setting) => { %>
            <li>
              <%= setting.name %> : <%= setting.content %> 
              <a href="/setting/edit/<%= setting.id %> ">編集</a>  
            </li>
        <% }) %>
    </ul>
  </body>
</html>